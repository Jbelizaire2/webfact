        <link rel="icon" type="image/x-icon" href="{{ asset('assets/main/image/logowebfact.png') }}" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
       
       
        <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/css/select2.min.css" rel="stylesheet" />
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous">
       
       
        {% block stylesheets %}{% endblock %}


         <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-Fy6S3B9q64WdZWQUiU+q4/2Lc9npb8tCaSX9FK7E8HnRr0Jz8D6OP9dO5Vg3Q9ct" crossorigin="anonymous"></script>

    <!-- jQuery 3 -->
<script src="{{ asset('build/jquery.min.js')}}"></script>

<!-- jQuery UI 1.11.4 -->
<script src="{{ asset('build/jquery-ui.min.js')}}"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/js/select2.min.js"></script>

    </head>
    <body>
        {% block body %}{% endblock %}
        <script type="text/javascript" src="{{ asset('assets/main/js/jquery-3.3.1.min.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/main/js/igrowl.js')}}"></script>
        {% block javascripts %}{% endblock %}
        {% for flash_message in app.session.flashBag.get('error') %}
            <script type="text/javascript">
                $(function() {
                    console.log('flag');
                    $.iGrowl({
                        title: 'Error',
                        message: '{{ flash_message }}',
                        type: 'error-sat',
                        icon: 'linecons-lock',
                        animShow: 'fadeInDown',
                        animHide: 'fadeOutRight',
                        placement : {
                        x:  'right',
                        y:  'top'
                        }
                    })

                    $('.alert').alert();
                });
                
            </script>
        {% endfor %}    
    </body>
